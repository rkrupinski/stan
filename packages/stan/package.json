{
  "name": "@rkrupinski/stan",
  "description": "Minimal, type-safe state management",
  "version": "1.3.3-rc5",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/react.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint --cache --max-warnings=0",
    "test": "jest",
    "build": "rm -rf dist && rollup --config",
    "prepublishOnly": "yarn typecheck && yarn lint && yarn test && yarn build"
  },
  "dependencies": {
    "fast-json-stable-stringify": "2.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@rkrupinski/prettierconfig": "1.0.1",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.2.0",
    "@types/jest": "29.5.14",
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.27.0",
    "eslint-import-resolver-typescript": "4.3.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "^16.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "rollup": "4.37.0",
    "ts-jest": "29.3.0",
    "tslib": "2.8.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "author": "Rafał Krupiński <rkrupinski.npm@gmail.com> (https://github.com/rkrupinski)",
  "homepage": "https://github.com/rkrupinski/stan#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkrupinski/stan.git"
  },
  "license": "MIT",
  "prettier": "@rkrupinski/prettierconfig"
}
